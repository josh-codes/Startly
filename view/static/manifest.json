{
    "manifest_version": 3,
    "name": "Starty",
    "githuburl": "https://github.com/josh-codes/Startly",
    "version": "0.0.1",
    "update_url": "https://clients2.google.com/service/update2/crx",
    "description": "A nice simple and beautiful startpage.",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png",
        "512": "icons/512.png"
    },
    "chrome_url_overrides" : {
        "newtab": "pages/index.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
  }