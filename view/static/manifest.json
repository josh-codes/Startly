{
    "manifest_version": 3,
    "name": "Starty",
    "version": "0.0.1",
    "description": "A nice simple and beautiful startpage.",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png",
        "512": "icons/512.png"
    },
    "chrome_url_overrides" : {
        "newtab": "pages/index.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "https://example.com/"
    ]
  }